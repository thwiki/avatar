{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"github": {
		"silent": true
	},
	"rewrites": [
		{
			"source": "/",
			"destination": "/default_l.jpg"
		},
		{
			"source": "/favicon.ico",
			"destination": "/favicon.ico"
		},
		{
			"source": "/robots.txt",
			"destination": "/robots.txt"
		},
		{
			"source": "/:path(.+[.](?:jpg|jpeg|png|gif))",
			"destination": "https://upload.thwiki.cc/avatars/:path"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [{ "key": "Cache-Control", "value": "max-age=315360000, s-maxage=86400, stale-while-revalidate=86400, public" }]
		}
	],
	"regions": ["sfo1"]
}
