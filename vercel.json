{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"github": {
		"silent": true
	},
	"rewrites": [
		{
			"source": "/",
			"destination": "/default_l.jpg"
		},
		{
			"source": "/favicon.ico",
			"destination": "/favicon.ico"
		},
		{
			"source": "/robots.txt",
			"destination": "/robots.txt"
		},
		{
			"source": "/:path(.+[.](?:jpg|jpeg|png|gif))",
			"destination": "/api"
		}
	],
	"regions": ["hnd1"]
}
