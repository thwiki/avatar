{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"declaration": false,
		"lib": ["es2020"],
		"moduleResolution": "node",
		"module": "esnext",
		"target": "es2020",
		"types": ["node"],
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	}
}
